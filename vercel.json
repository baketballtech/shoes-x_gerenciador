{
  "version": 2,
  "builds": [
    {
      "src": "/servidor_vercel.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "(.*)",
      "dest": "/servidor_vercel.js"
    },
    { "src": "/login", "dest": "/servidor_vercel.js" },
    { "src": "/procura", "dest": "/servidor_vercel.js" },
    { "src": "/sapatos", "dest": "/servidor_vercel.js" },
    { "src": "/change", "dest": "/servidor_vercel.js" },
    { "src": "/vendas", "dest": "/servidor_vercel.js" }
  ]
}
  
